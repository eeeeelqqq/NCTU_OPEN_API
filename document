OPENAPI 

[COURSES]

URL: ./api/courses
type: "GET"

data:
	option:
		real_id			int
		ch_name			varchar
		eng_name		varchar
		departmentid	int
		teacher_id		int


[MAIL]

URL: ./api/mail/		//redefine it
type: "GET"
RETURN: all mail

URL: ./api/mail/:departmentid/		[Detail]
type: "GET"

data:
	option:
		name 	varchar //收信人
		date 	{start, end}	datetime yyyy/mm/dd //查詢起訖時間
		type 	int

	RETURN:
		[{
			id: int
			departmentid: int
			name: varchar
			date: datetime yyyy/mm/dd
			type: int
			exist: bool
		}]

	type enum()

